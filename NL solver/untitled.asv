%%config
iterations = 10;
freq = linspace(3,9,201)*1e9;

nockit_params = get_nockit6_params();
inpu_idx= 4;
sig_pwr= -80; %dBm
G = get_nockit_graph_NL(nockit_params, input_idx,sig_pwr);
graph_data = process_graph_NL(G);
%%

for i=1:length(freq)
   [t_edges, r_edges] = solve_graph_NL_envelope(graph_data,freq, iterations);
   
   [t,r]   = read_nockit_solution(nockit_params, G,t_edges,r_edges
    
end